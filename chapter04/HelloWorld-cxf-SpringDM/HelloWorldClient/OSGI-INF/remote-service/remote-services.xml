<?xml version="1.0" encoding="UTF-8"?>
<service-descriptions xmlns="http://www.osgi.org/xmlns/sd/v1.0.0">
  <!-- 服务描述-->
  <service-description>
    <!--远程服务映射到本地的服务的接口-->
    <provide interface="org.osgichina.helloworldcxf.springdm.HelloWorldService" />
    <!-- 远程服务的属性 -->
    <property name="osgi.remote.interfaces">*</property>
    <property name="osgi.remote.configuration.type">pojo</property>
    <property name="osgi.remote.configuration.pojo.address">http://localhost:9000/org/osgichina/helloworldcxf/springdm/HelloWorldService</property>
  </service-description>
</service-descriptions>
